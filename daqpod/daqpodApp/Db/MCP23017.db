# ./tempApp/Db/MCP23017.db
record(stringin, "$(USER):Version") {
   field(DESC, "Version info")
   field(SCAN, "10 second")
   field(DTYP, "stream")
   field(INP, "@MCP23017.proto get_version $(PORT)")
}
record(ai, "$(USER):getA") {
   field(DESC, "Get port")
   field(SCAN, "10 second")
   field(DTYP, "stream")
   field(INP, "@MCP23017.proto get(A) $(PORT)")
}

record(ao, "$(USER):setA") {
   field(DESC, "Set port")
   field(DTYP, "stream")
   field(OUT, "@MCP23017.proto set(A) $(PORT)")
}
